	$("#printfriendly").click(function() {
		var s = document.location.search;
		if (!s)
			window.open(document.location.pathname + '?print=1', '', 'scrollbars=1');
		else
			window.open(document.location.pathname + s + '&print=1', '', 'scrollbars=1');
		return false;
	});
});
</script>
